<tr class='set workout_exercise_set_details' data-value='{{set}}'>
  <td class='workout_exercise_set_detail_td'>
    <div>
      <p class='sets'>Set {{set}}:</p>  
    </div>
  </td>

  <td class='reps workout_exercise_set_detail_td' data-value='{{reps}}' data-type='reps'>
    <p>{{reps}} reps</p>
    {{# own_workout}}

      <div>
        <a href="{{set_details_url}}" class='change_workout_exercise_set_details' data-direction='up'><i class='icon-chevron-up change_workout_exercise_set_details' data-direction='up'></i></a>

        <a href="{{set_details_url}}" class='change_workout_exercise_set_details' data-direction='down'><i class='icon-chevron-down change_workout_exercise_set_details' data-direction='down'></i></a>
      </div>

      </div>
    {{/ own_workout}}
  </td>


  <td class='weight workout_exercise_set_detail_td' data-value='{{weight}}' data-type='weight'>
    <p>{{weight}}kg</p>
    {{# own_workout}}

      <div>
        <a href="{{set_details_url}}" class='change_workout_exercise_set_details' data-direction='up'><i class='icon-chevron-up change_workout_exercise_set_details' data-direction='up'></i></a>

        <a href="{{set_details_url}}" class='change_workout_exercise_set_details' data-direction='down'><i class='icon-chevron-down change_workout_exercise_set_details' data-direction='down'></i></a>
      </div>

    {{/ own_workout}}
  </td>

  {{# own_workout }}
    <td><a href="{{set_details_url}}" class='delete_workout_exercise_set_details'>x</a></td>
  {{/ own_workout }}
</tr>