.modal.hide.fade.in#add_workout_exercise

  .modal-header

    %a.close{ "data-dismiss" => "modal" } x
    %h2 Add Exercise To Workout

  .modal-body

    .span2.tabbable.tabs-left.affix-left
      = render partial: "exercises/tags"

    .span4.exercise_list.left_of_affix
      %h3.tag_title All Exercises
      %br
      = render partial: "exercise", collection: @exercises